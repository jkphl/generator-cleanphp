{
  "name": "<%- vendor %>/<%- project %>",
  "description": "<%- description %>",<% if(website) { %>
  "homepage": "<%- website %>",<% } %>
  "minimum-stability": "<%- stability %>",
  "license": "<%- license %>",
  "authors": [
    {
      "name": "<%- authorName %>",<% if(authorEmail) { %>
      "email": "<%- authorEmail %>",<% } if(authorWebsite) { %>
      "homepage": "<%- authorWebsite %>",<% } %>
      "role": "Developer"
    }
  ],
  "support": {<% if(authorEmail) { %>
    "email": "<%- authorEmail %>"<% } %>
  },
  "config": {
    "sort-packages": true
  },
  "repositories": {
    "graph-composer": {
      "type": "git",
      "url": "https://github.com/jkphl/graph-composer"
    }
  },
  "require": {
    "php": "<%- php %>"
  }<% if(namespace) { %>,
  "autoload": {
    "psr-4": {
      "<%- namespace %>\\": "src/"
    }
  }<% } %>
}
